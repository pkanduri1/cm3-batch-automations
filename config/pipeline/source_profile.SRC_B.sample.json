{
  "source_system": "SRC_B",
  "description": "Production-style sample profile for SRC_B",
  "stages": {
    "ingest": {
      "enabled": true,
      "command": "echo '[SRC_B] ingest checks completed'"
    },
    "sqlloader": {
      "enabled": true,
      "log_file": "logs/sqlloader_SRC_B.log",
      "max_rejected": 5,
      "max_discarded": 0,
      "command": "echo '[SRC_B] SQL*Loader stage handled externally; using log evaluation'"
    },
    "java_batch": {
      "enabled": true,
      "command": "echo '[SRC_B] Java batch completed'"
    },
    "output_validation": {
      "enabled": true,
      "targets": [
        {
          "name": "p327",
          "file": "outputs/current/SRC_B/p327.txt",
          "mapping": "config/mappings/P327_full_in_sheet_order.json",
          "rules": "config/rules/p327_business_rules.json",
          "strict_fixed_width": true,
          "strict_level": "all",
          "report": "reports/SRC_B_p327_validation.html",
          "baseline_file": "outputs/baseline/SRC_B/p327.txt",
          "compare_report": "reports/SRC_B_p327_compare.html"
        },
        {
          "name": "p999",
          "file": "outputs/current/SRC_B/p999.txt",
          "mapping": "config/mappings/p999.json",
          "rules": "config/rules/p999_rules.json",
          "strict_fixed_width": false,
          "report": "reports/SRC_B_p999_validation.html"
        }
      ]
    }
  }
}
